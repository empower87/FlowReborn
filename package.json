{
  "name": "flowreborn",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "npm start --workspace=packages/server",
    "server": "npm run devthree --workspace=packages/server",
    "client": "npm start --workspace=packages/client",
    "client-build": "cd client && npm install && npm run build",
    "server-build": "cd server && npm install",
    "build": "npm i --workspaces && npm run build --workspace=client",
    "dev": "npm-run-all --parallel start client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/empower87/FlowReborn.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/empower87/FlowReborn/issues"
  },
  "build": "npm run build --workspaces --if-present && npm run test && rm -rf dist && npm run lint && tsc -b",
  "deploy": "vercel deploy --prod",
  "workspaces": [
    "./packages/client",
    "./packages/server"
  ],
  "homepage": "https://flow-reborn.onrender.com",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
